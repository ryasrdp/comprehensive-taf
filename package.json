{
  "name": "comprehensive-taf",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "prettier": "npx prettier --write \"**/*.{js,jsx}\"",
    "eslint": "npx eslint --format codeframe --color ./",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "npx bddgen --tags \"not @skip\" && npx playwright test",
    "test:tag": "npx bddgen && npx playwright test --grep",
    "test-headed:tag": "npx bddgen && npx playwright test --headed --grep",
    "test:college": "npx bddgen && npx playwright test features/addCollegeLocation.feature"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryasrdp/comprehensive-taf.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ryasrdp/comprehensive-taf/issues"
  },
  "homepage": "https://github.com/ryasrdp/comprehensive-taf#readme",
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.10",
    "@babel/eslint-parser": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/register": "^7.25.9",
    "@playwright/test": "^1.53.1",
    "@types/node": "^24.0.1",
    "date-fns": "^4.1.0",
    "eslint": "^9.23.0",
    "eslint-formatter-codeframe": "^7.32.1",
    "eslint-plugin-playwright": "^2.2.0",
    "fakerator": "^0.3.6",
    "gherkin-lint": "^4.2.2",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "playwright": "^1.53.1",
    "playwright-bdd": "^8.3.1"
  },
  "lint-staged": {
    "*.js": [
      "npm run prettier",
      "npm run eslint"
    ]
  }
}
